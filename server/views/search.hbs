
        <main>
            <form class="search" action="" method="">
                <input type="search" name="" placeholder="Search here...">
                <button type="submit">Search</button>
            </form>
            <section id="find-section">

                <!-- If there are any matching matches from the search, show each of them -->
                {{#each homes}}
                <div class="card">
                    <h3>{{name}}</h3>
                    <p>{{description}}</p>
                    <div class="cta-container"><a href="/{{id}}/details" class="det-link">Details</a></div>
                    <div class="card_circle"><img src="{{homeImage}}"></div>

                </div>
                {{else}}
                <div class="no-data-listing">
                    <p class="no-offer">No match was found for the submitted type...</p>
                </div>
                 {{/each}}
            </section>
        </main>
